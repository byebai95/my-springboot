<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bai.app.dao.UserDao">

    <select id="getUserInfoById" resultType="com.bai.app.model.entity.User">
        select *  from t_user where uid = #{uid}
    </select>


</mapper>